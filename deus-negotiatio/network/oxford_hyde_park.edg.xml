<?xml version="1.0" encoding="UTF-8"?>
<!-- Oxford-Hyde Park Intersection Edges -->
<edges xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
       xsi:noNamespaceSchemaLocation="http://sumo.dlr.de/xsd/edges_file.xsd">

    <!-- NB Approach (from S to C): 4 lanes -->
    <edge id="NB_in" from="S" to="C" numLanes="4" speed="16.67"/>
    <!-- NB Departure (from C to N): 3 lanes -->
    <edge id="NB_out" from="C" to="N" numLanes="3" speed="16.67"/>

    <!-- SB Approach (from N to C): 4 lanes -->
    <edge id="SB_in" from="N" to="C" numLanes="4" speed="16.67"/>
    <!-- SB Departure (from C to S): 3 lanes -->
    <edge id="SB_out" from="C" to="S" numLanes="3" speed="16.67"/>

    <!-- EB Approach (from E to C): 3 lanes -->
    <edge id="EB_in" from="E" to="C" numLanes="3" speed="16.67"/>
    <!-- EB Departure (from C to E): 3 lanes -->
    <edge id="EB_out" from="C" to="E" numLanes="3" speed="16.67"/>

    <!-- WB Approach (from W to C): 3 lanes -->
    <edge id="WB_in" from="W" to="C" numLanes="3" speed="16.67"/>
    <!-- WB Departure (from C to W): 3 lanes -->
    <edge id="WB_out" from="C" to="W" numLanes="3" speed="16.67"/>

</edges>
